{
  "name": "mantine-admin-pro",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --open",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview --host --open",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,tsx,ts}": [
      "npm run lint:fix"
    ]
  },
  "dependencies": {
